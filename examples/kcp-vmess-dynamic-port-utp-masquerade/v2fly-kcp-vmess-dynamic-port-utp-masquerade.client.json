{
  "log": {
    "loglevel": "debug"
  },
  "dns": {
    "servers": [
      "https://1.1.1.1/dns-query"
    ]
  },
  "routing": {
    "domainStrategy": "AsIs",
    "rules": [
      {
        "type": "field",
        "domainMatcher": "mph",
        "domains": [
          "ext:antizapret.dat:ZAPRETINFO"
        ],
        "outboundTag": "proxy"
      },
      {
        "type": "field",
        "port": "0-65535",
        "network": "tcp,udp",
        "protocol": [
          "http",
          "tls",
          "bittorrent"
        ],
        "outboundTag": "direct"
      }
    ]
  },
  "inbounds": [
    {
      "port": 1080,
      "listen": "127.0.0.1",
      "protocol": "socks",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "settings": {
        "auth": "noauth"
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "inconspicuous-domain.local",
            "port": 49152,
            "users": [
              {
                "id": "9d0a470f-aaac-46df-96f4-3e2b16963d39",
                "alterId": 0,
                "security": "auto",
                "level": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "mkcp",
        "kcpSettings": {
          "congestion": true,
          "header": {
            "type": "utp"
          }
        }
      },
      "tag": "proxy"
    },
    {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    }
  ]
}